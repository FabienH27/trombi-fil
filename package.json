{
  "name": "trombinoscope",
  "version": "1.0.0",
  "description": "Trombi-FIL project made with vue3",
  "scripts": {
    "client": "cd client && npm run dev",
    "server": "cd server && npm run start",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "install-all": "npm i && cd server && npm i && cd ../client && npm i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FabienH27/trombi-fil.git"
  },
  "keywords": [
    "vue3"
  ],
  "author": "fabienh27",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FabienH27/trombi-fil/issues"
  },
  "homepage": "https://github.com/FabienH27/trombi-fil#readme",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "fs-extra": "^11.2.0"
  }
}
